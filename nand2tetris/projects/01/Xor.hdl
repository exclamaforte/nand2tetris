/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=abnand);
    Nand(a=a, b=abnand, out=top);
    Nand(a=abnand, b=b, out=bottom);
    Nand(a=top, b=bottom, out=out);
}